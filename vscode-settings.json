// Place your settings in this file to overwrite the default settings
{
	"git.path": "c:/Dev/tools/Git/cmd/git.exe",
	// typescript
	"tslint.autoFixOnSave": false,
	// vim
	"vim.visualstar": true,
	"vim.enableNeovim": true,
	"vim.neovimPath": "nvim",
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<C-E>": false,
		"<C-F>": false,
		"<C-b>": false,
		"<C-G>": false,
		"<C-D>": false,
		"<C-X>": false,
		"<C-j>": false,
		"<C-k>": false
	},
	"vim.otherModesKeyBindingsNonRecursive": [
		{
			"before": ["D"], "after": ["y", "P"]
		},
		{
			"before": ["<leader>","p"], "after": ["\"","*","p"]
		},
		{
			"before": ["<leader>","y"], "after": ["\"","*","y"]
		},
		{
			"before": ["g","D"],
			"after": [],
			"commands": [
				{
					"command": "editor.action.goToTypeDefinition",
					"args": []
				}
			]
		},
		{
			"before": ["g","r"],
			"after": [],
			"commands": [
				{
					"command": "editor.action.referenceSearch.trigger",
					"args": []
				}
			]
		},
		{
			"before": ["<leader>", "g", "d"],
			"after": [],
			"commands": [
				{
					"command": "editor.action.previewDeclaration",
					"args": []
				}
			]
		},
		{
			"before": ["<leader>", "g", "D"],
			"after": [],
			"commands": [
				{
					"command": "editor.action.peekTypeDefinition",
					"args": []
				}
			]
		},
		{
			"before": ["<leader>","j"], "after": ["i", "<cr>", "<esc>"]
		},
		{
			"before": ["L"], "after": ["$"]
		},
		{
			"before": ["[", "<space>"], "after": ["O", "<esc>"]
		},
		{
			"before": ["]", "<space>"], "after": ["o", "<esc>"]
		},
		{
			"before": ["H"], "after": ["^"]
		},
		{
			"before": ["<leader>", "f", "s"],
			"after": [],
			"commands": [
				{
					"command": "workbench.action.files.save",
					"args": []
				}
			]
		},
		{
			"before": ["<leader>", "b", "d"],
			"after": [],
			"commands": [
				{
					"command": "workbench.action.closeActiveEditor",
					"args": []
				}
			]
		},
		{
			//"before": ["c", "o", "b"],
			"before": ["<leader>", "o", "b"],
			"after": [],
			"commands": [
				{
					"command": "workbench.action.toggleSidebarVisibility",
					"args": []
				}
			]
		}
	],
	"vim.disableAnnoyingNeovimMessage": true,

	"editor.minimap.enabled": false,
	"editor.fontFamily": "'Source Code Pro', Consolas, 'Courier New', monospace",
	"editor.fontSize": 16,
	"editor.fontLigatures": true,
	"editor.codeLens": false,
	"workbench.sideBar.location": "right",
	"window.menuBarVisibility": "toggle",
	"editor.renderWhitespace": "none",
	"window.zoomLevel": 0,
	"workbench.iconTheme": null,

	"debug.allowBreakpointsEverywhere": true,

	"FSharp.logLanguageServiceRequestsOutputWindowLevel": "DEBUG",
	"FSharp.logLanguageServiceRequests": "both",

	"workbench.activityBar.visible": false,
	"rust.rustup": {
		"toolchain": "stable-x86_64-pc-windows-msvc",
		"nightlyToolchain": "nightly-x86_64-pc-windows-msvc"
	},
	"typescript.check.tscVersion": false,
	"workspace": {
		"file:///c:/Dev/git/pushpay/Source/Pushpay.Public/JavaScript/LoggedInWeb": {
			"folders": [
				"file:///c:/Dev/git/pushpay/Source/Pushpay.Public/Content/LoggedInWeb"
			]
		}
	},

	"workbench.colorCustomizations": {
		"editorCursor.foreground": "#0a9dff",
		"editorCursor.background": "#0d1011",
		"selection.background": "#0a9dff",
		"terminal.ansiBrightWhite": "#ebdbb2",
