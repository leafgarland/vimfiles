		},
		"nuget": {
			"newestVersionsFirst": true,
			"includePreRelease": true,
			"disablePrefetch": false
		},
		"schemaVersion": 1
	},
	"workbench.editor.showTabs": true,
	"workbench.colorTheme": "Nord"
}
